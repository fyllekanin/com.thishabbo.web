    "test:watch": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng test --watch=true",
    "test:chrome": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng test --watch=true --browsers=Chrome",
