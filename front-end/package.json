{
    "name": "fansite",
    "version": "0.0.10",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --live-reload --proxy-config proxy.conf.json",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test",
        "test:watch": "ng test --watch=true",
        "test:chrome": "ng test --watch=true --browsers=Chrome",
        "lint": "ng lint",
        "e2e": "webdriver-manager update && node ./node_modules/protractor/bin/protractor ./e2e/protractor.conf.js",
        "e2e:jenkins": "webdriver-manager update && node ./node_modules/protractor/bin/protractor ./e2e/protractor-jenkins.conf.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "7.1.4",
        "@angular/common": "7.1.4",
        "@angular/compiler": "7.1.4",
        "@angular/core": "7.1.4",
        "@angular/forms": "7.1.4",
        "@angular/http": "7.1.4",
        "@angular/platform-browser": "7.1.4",
        "@angular/platform-browser-dynamic": "7.1.4",
        "@angular/router": "7.1.4",
        "core-js": "~2.6.1",
        "d3": "~5.7.0",
        "rxjs": "~6.3.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.12.2",
        "@angular/cli": "^7.0.6",
        "@angular/compiler-cli": "7.1.4",
        "@angular/language-service": "7.1.4",
        "@types/d3": "^5.0.1",
        "@types/jasmine": "^3.3.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^10.12.10",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~3.3.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "3.1.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-coverage-istanbul-reporter": "2.0.4",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.4.0",
        "karma-junit-reporter": "1.2.0",
        "karma-phantomjs-launcher": "~1.0.4",
        "karma-spec-reporter": "0.0.32",
        "karma-trx-reporter": "^0.4.0",
        "protractor": "~5.4.1",
        "protractor-jasmine2-screenshot-reporter": "^0.5.0",
        "terser": "3.14.1",
        "ts-mockito": "^2.3.1",
        "ts-node": "~7.0.0",
        "tslint": "~5.12.0",
        "typescript": "~3.1.0"
    }
