{
  "name": "fansite",
  "version": "0.2.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:watch": "ng test --watch=true",
    "test:chrome": "ng test --watch=true --browsers=Chrome",
    "lint": "ng lint",
    "e2e": "webdriver-manager update && node ./node_modules/protractor/bin/protractor ./e2e/protractor.conf.js",
    "e2e:jenkins": "webdriver-manager update --versions.chrome 2.46 && node ./node_modules/protractor/bin/protractor ./e2e/protractor-jenkins.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.14",
    "@angular/common": "7.2.14",
    "@angular/compiler": "7.2.14",
    "@angular/core": "7.2.14",
    "@angular/forms": "7.2.14",
    "@angular/http": "7.2.14",
    "@angular/platform-browser": "7.2.14",
    "@angular/platform-browser-dynamic": "7.2.14",
    "@angular/router": "7.2.14",
    "core-js": "~2.6.1",
    "d3": "~5.9.2",
    "jquery": "2.2.2",
    "rxjs": "~6.3.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "7.2.14",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "11.13.6",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~5.4.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "~3.1.0"
  }
}
